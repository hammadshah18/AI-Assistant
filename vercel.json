{
  "version": 3,
  "projects": [
    {
      "src": "Frontend/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "Backend/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "Backend/main.py" },
    { "src": "/(.*)", "dest": "Frontend/$1" }
  ]
}
